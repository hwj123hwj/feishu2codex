# Feishu App Credentials
# Get these from Feishu Developer Console -> Credentials & Basic Info
FEISHU_APP_ID=
FEISHU_APP_SECRET=

# Feishu SDK Logger Level (debug, info, warn, error)
FEISHU_LOGGER_LEVEL=info
# Feishu Bot Open ID (required for safe group @ mention filtering)
FEISHU_BOT_OPEN_ID=

# --- Codex Configuration ---

# Sandbox Mode
# Options: "read-only", "workspace-write", "danger-full-access"
CODEX_SANDBOX_MODE=workspace-write

# Approval Policy
# Options: "never", "on-request", "on-failure", "untrusted"
CODEX_APPROVAL_POLICY=never

# Skip Git Repo Check
CODEX_SKIP_GIT_CHECK=true

# Model Reasoning Effort
# Options: "minimal", "low", "medium", "high", "xhigh"
CODEX_REASONING_EFFORT=medium

# Model name (must be a model your account can access)
# Example: gpt-5-codex
# CODEX_MODEL=

# Web Search Enabled
CODEX_WEB_SEARCH_ENABLED=true

# Working Directory (optional)
# CODEX_WORKING_DIRECTORY=

# Run timeout for each Codex turn in milliseconds
CODEX_RUN_TIMEOUT_MS=3000000

# --- Web Console Security ---
# Bind address for web server (default localhost only)
WEB_HOST=127.0.0.1
# Web console port
WEB_PORT=3000
# Optional API token for /api/* endpoints (Authorization: Bearer <token>)
WEB_API_TOKEN=

# --- MCP Server Configuration ---

# GitHub Personal Access Token (for GitHub MCP)
GITHUB_PERSONAL_ACCESS_TOKEN=

